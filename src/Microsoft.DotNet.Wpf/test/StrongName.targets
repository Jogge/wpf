<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup Condition="'$(SignAssembly)' == 'true' AND '$(AssemblyOriginatorKeyFile)' == '$(TestTrustedKey)'" >
        <PublicKey>$(TestTrustedPublicKey)</PublicKey>
        <PublicKeyToken>$(TestTrustedPublicKeyToken)</PublicKeyToken>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(SignAssembly)' == 'true' AND '$(AssemblyOriginatorKeyFile)' == '$(TestUntrustedKey)'" >
        <PublicKey>$(TestUntrustedPublicKey)</PublicKey>
        <PublicKeyToken>$(TestUntrustedPublicKeyToken)</PublicKeyToken>
  </PropertyGroup>
</Project>
