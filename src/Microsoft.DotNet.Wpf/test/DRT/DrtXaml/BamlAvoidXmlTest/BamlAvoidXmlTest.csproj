<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>BamlAvoidXmlTest</AssemblyName>
    <LinkSubsystem>console</LinkSubsystem>
    <AssemblyIdentityVersionName>WCP</AssemblyIdentityVersionName>
    <GenerateAssemblyAttribute>false</GenerateAssemblyAttribute>
    <AddTargetFrameworkAttributeToWpfTestExe>true</AddTargetFrameworkAttributeToWpfTestExe>

    <!-- Avoid auto-generating a main -->
    <InternalMarkupCompilation>true</InternalMarkupCompilation>
    <RootNamespace>BamlAvoidXmlTest</RootNamespace>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <PropertyGroup>
    <StartupObject>BamlAvoidXmlTest.Entry</StartupObject>
    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
    <Compile Include="BamlAvoidXmlTest.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="$(WpfSourceDir)System.Xaml\System.Xaml.csproj" />
  </ItemGroup>
</Project>
