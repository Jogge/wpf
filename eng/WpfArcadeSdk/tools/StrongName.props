<Project>
    <PropertyGroup>
        <SignAssembly>false</SignAssembly>
        <TestUntrustedKey>$(WpfTestBasePath)\Common\keys\TestUntrusted.snk</TestUntrustedKey>
        <TestUntrustedPublicKey></TestUntrustedPublicKey>
        <TestUntrustedPublicKeyToken>A069DEE354D95F76</TestUntrustedPublicKeyToken>

        <TestTrustedKey>$(WpfTestBasePath)\Common\keys\TestTrusted.snk</TestTrustedKey>
        <TestTrustedPublicKey></TestTrustedPublicKey>
        <TestTrustedPublicKeyToken>e1e6160fdd198bb1</TestTrustedPublicKeyToken>
        
        <!-- Sign with the test key and ensure that we do not delay sign test code -->
        <DelaySign>false</DelaySign>
        <AssemblyOriginatorKeyFile Condition="'$(AssemblyOriginatorKeyFile)'==''">$(TestUntrustedKey)</AssemblyOriginatorKeyFile>
        <StrongNameKeyId>custom</StrongNameKeyId>
    </PropertyGroup>
</Project>